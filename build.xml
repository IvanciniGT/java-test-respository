  <property name="project.jdk.home" value="${jdk.home.10}"/>
  <property name="project.jdk.bin" value="${jdk.bin.10}"/>
  <property name="project.jdk.classpath" value="jdk.classpath.10"/>
  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <fileset dir="${javac2.home}">
      <include name="javac2.jar"/>
      <include name="jdom.jar"/>
      <include name="asm-all*.jar"/>
      <include name="jgoodies-forms.jar"/>
    </fileset>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  
  <!-- Module java-test-respository -->
  
  <dirname property="module.java-test-respository.basedir" file="${ant.file}"/>
  
  
  <property name="module.jdk.home.java-test-respository" value=""/>
  <property name="module.jdk.bin.java-test-respository" value=""/>
  <property name="module.jdk.classpath.java-test-respository" value=""/>
  
  <property name="compiler.args.java-test-respository" value="-encoding UTF-8 -source 9 -target 9 ${compiler.args}"/>
  
  <property name="java-test-respository.output.dir" value="undefined"/>
  <property name="java-test-respository.testoutput.dir" value="undefined"/>
  
  <path id="java-test-respository.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="java-test-respository.module.production.classpath"/>
  
  <path id="java-test-respository.runtime.production.module.classpath"/>
  
  <path id="java-test-respository.module.classpath"/>
  
  <path id="java-test-respository.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.java-test-respository">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.java-test-respository">
    <patternset refid="excluded.from.module.java-test-respository"/>
  </patternset>
  
  
  <target name="compile.module.java-test-respository" depends="compile.module.java-test-respository.production,compile.module.java-test-respository.tests" description="Compile module java-test-respository"/>
  
  <target name="compile.module.java-test-respository.production" depends="register.custom.compilers" description="Compile module java-test-respository; production classes"/>
  
  <target name="compile.module.java-test-respository.tests" depends="register.custom.compilers,compile.module.java-test-respository.production" description="compile module java-test-respository; test classes" unless="skip.tests"/>
  
  <target name="clean.module.java-test-respository" description="cleanup module">
    <delete dir="${java-test-respository.output.dir}"/>
    <delete dir="${java-test-respository.testoutput.dir}"/>
  </target>
  
  
