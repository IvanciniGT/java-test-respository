  <target name="clean.module.java-test-respository1" description="cleanup module">
    <delete dir="${java-test-respository1.output.dir}"/>
    <delete dir="${java-test-respository1.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.java-test-respository, clean.module.java-test-respository1" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.java-test-respository, compile.module.java-test-respository1" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
